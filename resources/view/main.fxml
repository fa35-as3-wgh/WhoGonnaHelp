<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<TabPane maxHeight="480.0" maxWidth="640.0" minHeight="480.0" minWidth="640.0" prefHeight="480.0" prefWidth="640.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fa35.group2.view.gui.MainController">
  <tabs>
      <Tab onSelectionChanged="#load_view" text="Ansicht">
          <content>
          <fx:include fx:id="view" source="view.fxml" />
          </content>
      </Tab>
      <Tab onSelectionChanged="#load_edit" text="Bearbeiten / Hinzufügen">
          <content>
              <fx:include fx:id="edit" source="edit.fxml" />
          </content>
      </Tab>
      <Tab onSelectionChanged="#load_adddelete" text="Fähigkeiten / Bezahlmethoden">
          <content>
              <fx:include fx:id="adddelete" source="adddelete.fxml" />
          </content>
      </Tab>
  </tabs>
</TabPane>
